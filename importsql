CREATE TABLE infomsg (     msg_id TEXT PRIMARY KEY,                          gru_id TEXT NOT NULL,                                msg_numreferencia TEXT NOT NULL,                     msg_titportugues TEXT NOT NULL,                               msg_infportugues TEXT NOT NULL,                            msg_titespanhol TEXT NOT NULL,                              msg_infespanhol TEXT NOT NULL,                          msg_titingles TEXT NOT NULL,                               msg_infingles TEXT NOT NULL,                         msg_titfrances TEXT NOT NULL,                              msg_inffrances TEXT NOT NULL,                          criado_em TIMESTAMP DEFAULT CURRENT_TIMESTAMP,         FOREIGN KEY (gru_id) REFERENCES tipogrupo(gru_id)    );
